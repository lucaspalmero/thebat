        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (room_is_active){
    end_x = mouse_x;
    end_y = mouse_y;
    
    if (abs(end_x - start_x) &gt; 32 or abs(end_y - start_y) &gt; 32){
        //Si se hizo un slide mayor a 32px
        angle = scr_getAngle(start_x, start_y, end_x, end_y);
        
        if (scr_batCanMove(angle, bat)){
            //Si angulo_obtenido != -1 (no puede moverse)
            //  Mover jugador
            if (bat.speed == 0 and angle != 0){
                bat.sprite_index = spr_bat_fly;
                bat.image_speed = 0.3;
                bat.direction = angle;
                bat.mov_speed = 8;
            }
        }
    }else{
        if (!sonar and bat.speed == 0){
            //Trazar una linea desde obj_bat hacia el punto señalado y obtener angulo
            //Sonar ecolocalización
            aim_to_mouse = scr_getAngle(bat.x, bat.y, start_x, start_y);
            sonar = true;
            scr_echo(aim_to_mouse);
        }
    }
}
</string>
          </argument>
        </arguments>
      </action>
